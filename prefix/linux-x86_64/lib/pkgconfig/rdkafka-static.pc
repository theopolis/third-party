prefix=/vagrant/third-party/prefix/linux-x86_64
libdir=/vagrant/third-party/prefix/linux-x86_64/lib
includedir=/vagrant/third-party/prefix/linux-x86_64/include

Name: librdkafka-static
Description: The Apache Kafka C/C++ library (static)
Version: 1.0.1
Cflags: -I${includedir}
Libs: -L${libdir} ${libdir}/librdkafka.a -lm -Wl,-Bstatic -lssl -Wl,-Bdynamic -Wl,-Bstatic -lcrypto -Wl,-Bdynamic -Wl,-Bstatic -lz -Wl,-Bdynamic -ldl -lpthread -lrt -lcrypto
